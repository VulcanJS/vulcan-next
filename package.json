    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.0",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest": "^25.2.3",
    "auto-changelog": "^2.2.1",
    "cypress": "4.8.0",
    "eslint-plugin-cypress": "2.11.1",
    "jest": "^26.0.1",
    "jest-transformer-mdx": "^2.2.0",
    "nyc": "^15.1.0",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.11.0"
  }
}
